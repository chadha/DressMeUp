Peerius.changeTracker=function(retriever,checker){
	var original=null;
	var intervalHandler=null;
	var f=function(){
		var s=retriever();
		var o=original;
		original=s;
		if(o!=null && s!=o) 
		{
			if(checker && !checker(s)) return;
			clearInterval(intervalHandler);
			Peerius.dynamic();
			setTimeout(function(){
				original=retriever();
				intervalHandler=setInterval(f,500)
			},1000)
		}
	}
	
	// wait for the page layout to finalize
	setTimeout(function(){
		intervalHandler=setInterval(f,500)
	},500)
}

